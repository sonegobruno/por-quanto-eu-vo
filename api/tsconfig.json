{
  "compilerOptions": {
    "lib": [
        "es5",
        "es6",
        "ES2018"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
